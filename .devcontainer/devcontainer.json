{
    "name": "workspace",
    "dockerComposeFile": [
        "docker-compose.yml"
    ],
    "service": "workspace",
    "workspaceFolder": "/home/vscode/workspace", //workspace in container
    "remoteUser": "vscode",//because we build docker with user vscode.
    "shutdownAction": "stopCompose",
    "customizations": {
        "vscode": {
            "extensions": [
                "vscjava.vscode-java-pack",
                "vscjava.vscode-spring-initializr",
                "vscjava.vscode-maven"
            ]
        }
    }
}